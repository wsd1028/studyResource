                                    color: "#fff",
                                    fontSize: 12
                                }
                            },
                            series: {
                                name: _this.echartName,
                                type: 'gauge',
                                splitNumber: 1, //刻度数量
                                min: 0,
                                max: _this.data1.ipSum,
                                textStyle: {
                                    fontSize: 12
                                },
                                center: ['50%', '50%'],
                                radius: '90%',
                                detail: {
                                    offsetCenter: [0, 0],
                                    formatter: function () {
                                        return dataModule.isRealData === false ? '空闲{a|' + _this.data1.percent + '}%' : '空闲{a|' + _this.data1.percent + '}个';
                                    }, 
                                    textStyle: {
                                        fontSize: 14,
                                        color: '#fff'
                                    },
                                    rich: {
                                        a: {
                                            fontSize: 24,
                                            color: '#fff'
                                        }
                                    }
                                },
                                title: {
                                    textStyle: {
                                        color: "#fff"
                                    },
                                    offsetCenter: [0, '70%']
                                },
                                axisLine: {
                                    show: false,
                                    lineStyle: { // 属性lineStyle控制线条样式 (_data.Data[0].FluxData.Flux / _data.Data[0].FluxData.Speed).toFixed(1)
                                        color: [
                                            [(_this.data1.ipUsed / _this.data1.ipSum).toFixed(1), '#12a87f'],
                                            [1, '#55639a']
                                        ],
                                        width: 15
                                    }
                                },
                                axisTick: {
                                    show: false,
                                    lineStyle: {
                                        color: "#fff",
                                        width: 1
                                    },
                                    length: -2,
                                    splitNumber: 3
                                },
                                pointer: {
                                    show: false
                                },
                                splitLine: {
                                    show: false,
                                    length: -5,
                                    lineStyle: {
                                        color: "#fff"
                                    }
                                },
                                axisLabel: {
                                    formatter: function (value) {
                                        return dataModule.isRealData === false ? (parseInt(value) / _this.data1.ipSum).toFixed(2) * 100 + '%' : '    ' + value;
                                    },
                                    distance: -25,
                                    textStyle: {
                                        color: "#fff",
                                        fontSize: '14'
                                    }
